<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" attributeFormDefault="unqualified">
	<xs:element name="IbrFilterConfig">
		<xs:complexType>
		<xs:annotation>
			<xs:documentation>(U) Parameters for configuration of the IBR filter</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="radius" type="xs:int">
				<xs:annotation>
					<xs:documentation>Ownship Filter radius, 1-4095 nm.  Used in GLF_Circle struct.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TOI_Window" type="xs:int">
				<xs:annotation>
					<xs:documentation>Time Of Intercept (TOI) filter, used only for reports containing Correlation Indices (CIs) not in the CI list. 0-255, 0=no restriction.  Used in LOGON and Filter Status structs.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	</xs:element>
</xs:schema>